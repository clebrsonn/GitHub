<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">
	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>searchCategory</from-outcome>
			<to-view-id>/category/searchCat.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>


	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>updateCategory</from-outcome>
			<to-view-id>/category/updateCat.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>deleteCategory</from-outcome>
			<to-view-id>/category/deleteCat.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>searchProduct</from-outcome>
			<to-view-id>/product/searchProd.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>


	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>updateProduct</from-outcome>
			<to-view-id>/product/updateProd.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>deleteProduct</from-outcome>
			<to-view-id>/product/deleteProd.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>
	</navigation-rule>


	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>searchPedido</from-outcome>
			<to-view-id>/pedido/searchPed.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>deletePedido</from-outcome>
			<to-view-id>/pedido/deletePed.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>seeOrder</from-outcome>
			<to-view-id>/pedido/closePed.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>SearchName</from-outcome>
			<to-view-id>/public/searchResult.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>



</faces-config>